{
    "name":  "json-table",
    "version":  "0.1.0",
    "private":  false,
    "description":  "Embeddable JSON ↔ Table widget (UMD global: JsonTable)",
    "type":  "module",
    "main":  "dist/jsontable.umd.cjs",
    "module":  "dist/jsontable.es.js",
    "types":  "dist/types/index.d.ts",
    "exports":  {
                    ".":  {
                              "import":  "./dist/jsontable.es.js",
                              "require":  "./dist/jsontable.umd.cjs",
                              "types":  "./dist/types/index.d.ts"
                          }
                },
    "scripts":  {
                    "preview":  "vite preview",
                    "test":  "vitest run",
                    "build:demo:local":  "vite build demo --outDir dist",
                    "build":  "vite build \u0026\u0026 tsc -p tsconfig.build.json",
                    "lint":  "eslint .",
                    "typecheck":  "tsc --noEmit",
                    "build:demo":  "vite build demo --outDir dist-demo --base=/json-table/",
                    "dev":  "vite",
                    "preview:demo":  "node scripts/serve-demo.mjs"
                },
    "keywords":  [
                     "json",
                     "table",
                     "grid",
                     "widget",
                     "diff",
                     "flatten"
                 ],
    "author":  "",
    "license":  "MIT",
    "devDependencies":  {
                            "@typescript-eslint/eslint-plugin":  "^7.16.1",
                            "@typescript-eslint/parser":  "^7.16.1",
                            "eslint":  "^8.57.0",
                            "jsdom":  "^24.0.0",
                            "typescript":  "^5.5.0",
                            "vite":  "^5.4.0",
                            "vitest":  "^2.0.5"
                        }
}
